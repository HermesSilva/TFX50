    </Compile>
    <Compile Update="TFX.Core.Access.Module.cs">
      <Visible>False</Visible>
      <DependentUpon>TFX.Core.Access.dsmdl</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="TFX.Core.Access.Module.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>TFX.Core.Access.dsmdl</DependentUpon>
      <Visible>False</Visible>
    </Compile>
  </ItemGroup>

</Project>
