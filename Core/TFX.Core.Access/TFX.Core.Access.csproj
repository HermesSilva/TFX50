<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <RootNamespace>TFX.Access</RootNamespace>
      <MyType>DASE</MyType>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="TAF.Access.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="TFX.Core.Access.dsmdl" />
